    "node-sass": "^9.0.0",
    "rtlcss": "^3.5.0",
    "nodemon": "^3.0.1"
  },
  "rtlcssConfig": {
    "options": {
      "autoRename": false,
      "autoRenameStrict": false,
      "blacklist": {},
      "clean": true,
      "greedy": false,
      "processUrls": false,
      "stringMap": []
    },
    "plugins": [],
    "map": false
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "css": "node-sass --include-path scss scss/main.scss css/main.css",
    "watch": "nodemon --watch scss --ext scss --exec \"yarn css\""
  }
